package com.basicConcept;

public class ArrayDemo {
	
	public static void main(String[] args) {
		int []  arrayOfIntegers= new int[] {5,6,7};
		int [] array= {2,4,6,8,10};
		int [][] multidimensionalArray= {
				{1,2},
				{3,4}
		};
		
		System.out.println(array[0]);
		System.out.println(array[0]);
		System.out.println(array[0]);
		System.out.println(array[0]);
	}

}
