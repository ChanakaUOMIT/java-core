package com.basicConcept;

public class Calculator {
	public Integer add (Integer arg1, Integer arg2) {
		Integer result = arg1+arg2;
		return result;
	}
	public Integer add (Integer arg1, Integer arg2, Integer arg3) {
		Integer result = arg1+arg2+arg3;
		return result;
	}
	
	public static void main(String[] args) {
		Calculator cal = new Calculator();
		int arg1=10;
		int arg2=30;
		int arg3=40;
		Integer result = cal.add(arg1, arg2);
		System.out.println("Result : Calculator : " + result);
		
		Integer result_add = cal.add(arg1, arg2, arg3);
		System.out.println("Result_add : Calculator : " + result_add);
	}

}
